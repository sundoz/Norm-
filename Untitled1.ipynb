    "n=int(input())\n",
    "\n",
    "a=1\n",
    "cont=0\n",
    "i=1\n",
    "while i<=n:\n",
    "    if cont<a:\n",
    "        print(a)\n",
    "        cont+=1\n",
    "    else:\n",
    "        cont=0\n",
    "        a+=1\n",
    "    i+=1"
